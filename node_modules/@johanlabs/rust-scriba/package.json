{
  "name": "@johanlabs/rust-scriba",
  "version": "0.5.7",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "napi build --release",
    "test": "node test.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.10.0"
  },
  "author": "Johan Labs",
  "description": "Multi-tenant, high-performance asynchronous cache library for Node.js. Uses a persistent, sharded SQLite backend (WAL mode) with automatic checkpointing and per-key TTL support, offering a drop-in alternative to Redis for many caching scenarios.",
  "napi": {
    "name": "rust-scriba",
    "binaryName": "rust-scriba",
    "triples": [
      "x86_64-unknown-linux-gnu",
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc"
    ]
  },
  "files": [
    "index.js",
    "benchmark.js",
    "better-sqlite3.js",
    "package.json",
    "rust-scriba.node",
    "dist/**"
  ],
  "dependencies": {
    "perf_hooks": "^0.0.1",
    "redis": "^5.10.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
